{
  "rework": [
    {
      "activity": "assess_risk",
      "max_iteration": 1,
      "probability": 1,
      "condition": "loanType == 'personal'"
    }
  ],
  "long_rework": [
    {
      "trigger_activity": "credit_check",
      "back_to": "review_application",
      "max_iteration": 1,
      "probability": 1,
      "condition": "loanType == 'personal'"
    }
  ],
  "case_attributes": {
    "requestedAmount": {
      "type": "numerical",
      "min": 1000,
      "max": 10000
    },
    "loanType": {
      "type": "string",
      "values": ["personal", "mortgage", "auto"]
    },
    "isUrgent": {
      "type": "boolean"
    }
  }
}